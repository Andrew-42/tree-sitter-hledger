{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "hledger",
      "camelcase": "Hledger",
      "title": "Hledger",
      "scope": "source.hledger",
      "file-types": [
        "j",
        "journal"
      ],
      "injection-regex": "^hledger$",
      "class-name": "TreeSitterHledger"
    }
  ],
  "metadata": {
    "version": "1.0.0",
    "license": "MIT",
    "description": "Hledger grammar for tree-sitter",
    "authors": [
      {
        "name": "Andrew42"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-hledger"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
